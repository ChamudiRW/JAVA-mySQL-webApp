<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
  id="WebApp_ID" version="3.1">
  <display-name>BookingDemo_</display-name>
  

<servlet>
<servlet-name>display</servlet-name>
<servlet-class>com.VRS.bookingServlet</servlet-class>
</servlet>
  
<servlet-mapping>
<servlet-name>display</servlet-name>
<url-pattern>/display</url-pattern>
</servlet-mapping>




<servlet>
<servlet-name>insert</servlet-name>
<servlet-class>com.VRS.BookInsertServlet</servlet-class>
</servlet>
  
<servlet-mapping>
<servlet-name>insert</servlet-name>
<url-pattern>/insert</url-pattern>
</servlet-mapping>



<servlet>
<servlet-name>update</servlet-name>
<servlet-class>com.VRS.BookUpdateServlet</servlet-class>
</servlet>
  
<servlet-mapping>
<servlet-name>update</servlet-name>
<url-pattern>/update</url-pattern>
</servlet-mapping>
 
 
 
 
<servlet>
<servlet-name>delete</servlet-name>
<servlet-class>com.VRS.BookDeleteServlet</servlet-class>
</servlet>
  
<servlet-mapping>
<servlet-name>delete</servlet-name>
<url-pattern>/delete</url-pattern>
</servlet-mapping>





</web-app>


  